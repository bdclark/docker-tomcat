<?xml version='1.0' encoding='utf-8'?>
<!-- Dynamically generated by Consul-Template -->
<tomcat-users>
{{ range $roles := printf "%s/roles" ( env "TOMCAT_PREFIX" ) | key | split "," }}
  <role rolename="{{ $roles }}" />{{ end }}
{{ range $key, $pairs := printf "%s/users/" ( env "TOMCAT_PREFIX" ) | tree | byKey }}
  <user username="{{ $key }}" {{ range $pair := $pairs }}{{.Key}}="{{.Value}}" {{ end }}/>{{ end }}
</tomcat-users>
